#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/harshit.bhutani-mbp/Documents/Projects/next-shop/node_modules/.pnpm/eslint@8.24.0/node_modules/eslint/bin/node_modules:/Users/harshit.bhutani-mbp/Documents/Projects/next-shop/node_modules/.pnpm/eslint@8.24.0/node_modules/eslint/node_modules:/Users/harshit.bhutani-mbp/Documents/Projects/next-shop/node_modules/.pnpm/eslint@8.24.0/node_modules:/Users/harshit.bhutani-mbp/Documents/Projects/next-shop/node_modules/.pnpm/node_modules:/Users/harshit.bhutani-mbp/Documents/Projects/next-shop/node_modules:/Users/harshit.bhutani-mbp/Documents/Projects/node_modules:/Users/harshit.bhutani-mbp/Documents/node_modules:/Users/harshit.bhutani-mbp/node_modules:/Users/node_modules:/node_modules:/Users/harshit.bhutani-mbp/Documents/Projects/next-shop/node_modules/.pnpm/eslint-utils@3.0.0_eslint@8.24.0/node_modules/eslint-utils/node_modules:/Users/harshit.bhutani-mbp/Documents/Projects/next-shop/node_modules/.pnpm/eslint-utils@3.0.0_eslint@8.24.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/harshit.bhutani-mbp/Documents/Projects/next-shop/node_modules/.pnpm/eslint@8.24.0/node_modules/eslint/bin/node_modules:/Users/harshit.bhutani-mbp/Documents/Projects/next-shop/node_modules/.pnpm/eslint@8.24.0/node_modules/eslint/node_modules:/Users/harshit.bhutani-mbp/Documents/Projects/next-shop/node_modules/.pnpm/eslint@8.24.0/node_modules:/Users/harshit.bhutani-mbp/Documents/Projects/next-shop/node_modules/.pnpm/node_modules:/Users/harshit.bhutani-mbp/Documents/Projects/next-shop/node_modules:/Users/harshit.bhutani-mbp/Documents/Projects/node_modules:/Users/harshit.bhutani-mbp/Documents/node_modules:/Users/harshit.bhutani-mbp/node_modules:/Users/node_modules:/node_modules:/Users/harshit.bhutani-mbp/Documents/Projects/next-shop/node_modules/.pnpm/eslint-utils@3.0.0_eslint@8.24.0/node_modules/eslint-utils/node_modules:/Users/harshit.bhutani-mbp/Documents/Projects/next-shop/node_modules/.pnpm/eslint-utils@3.0.0_eslint@8.24.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@8.24.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../eslint@8.24.0/node_modules/eslint/bin/eslint.js" "$@"
fi
